{
	"name": "painperdu-bakery",
	"displayName": "PainPerdu Bakery",
	"description": "Color highlighting and autocomplete for PainPerdu language",
	"icon": "https://github.com/Baduit/PainPerdu/blob/master/vscode_extention/painperdu-bakery/assets/DALLE_2023-01-21_23.58.17_-_Pain_perdu_waifu_minimalist_digital_art.png",
	"version": "0.0.1",
	"repository": "https://github.com/Baduit/PainPerdu",
	"publisher": "Baduit",
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Programming Languages"
	],
	"activationEvents": [],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "PainPerdu.bake",
				"title": "Bake"
			}
		],
		"languages": [
			{
				"id": "painperdu",
				"aliases": [
					"PainPerdu",
					"Painperdu",
					"painperdu"
				],
				"extensions": [
					".pain"
				],
				"configuration": "./language-configuration.json",
				"icon": {
					"dark": "./assets/file_icon.png",
					"light": "./assets/file_icon.png"
				}
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.74.0",
		"@types/glob": "^8.0.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "16.x",
		"eslint": "^8.28.0",
		"glob": "^8.0.3",
		"mocha": "^10.1.0",
		"typescript": "^4.9.3",
		"@vscode/test-electron": "^2.2.0"
	},
	"configurationDefaults": {
		"painperdu": {
			"editor.semanticHighlighting.enabled": true
		}
	}
}